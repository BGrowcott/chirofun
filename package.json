{
  "name": "chirofun",
  "version": "1.0.0",
  "description": "Informational website for a Chiropractor",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js --ignore client",
    "dev": "concurrently \"cd server && nodemon\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BGrowcott/chirofun.git"
  },
  "author": "Ben Growcott",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BGrowcott/chirofun/issues"
  },
  "homepage": "https://github.com/BGrowcott/chirofun#readme",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "react-router-dom": "^6.26.0"
  }
}
